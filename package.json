{
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:tailwind": "concurrently 'next dev --turbo' 'npm run tailwind --watch'",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tailwind": "tailwindcss -i styles/globals.css -o styles/dist.css",
    "format": "prettier --write \"**/*.{js,ts,tsx,md}\"",
    "postinstall": "npm run tailwind"
  },
  "prettier": {
    "arrowParens": "always",
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@hookform/resolvers": "^3.1.0",
    "clsx": "^1.2.1",
    "concurrently": "^8.0.1",
    "next": "^13.3.1",
    "ofetch": "^1.0.1",
    "react": "next",
    "react-dom": "next",
    "react-hook-form": "^7.43.9",
    "react-loading": "^2.0.3",
    "styled-components": "^6.0.0-beta.15",
    "styled-jsx": "^5.1.2",
    "styled-reset": "^4.4.6",
    "use-count-up": "^3.0.1",
    "usehooks-ts": "^2.9.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/node": "^18.16.0",
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.39.0",
    "eslint-config-next": "^13.3.1",
    "postcss": "^8.4.23",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.4"
  }
}
