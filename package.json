{
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:tailwind": "concurrently 'next dev --turbo' 'npm run tailwind --watch'",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tailwind": "tailwindcss -i styles/globals.css -o styles/dist.css",
    "format": "prettier --write \"**/*.{js,ts,tsx,md}\"",
    "postinstall": "npm run tailwind"
  },
  "prettier": {
    "arrowParens": "always",
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.1",
    "clsx": "^2.0.0",
    "concurrently": "^9.0.0",
    "next": "^14.0.0",
    "ofetch": "^1.3.3",
    "react": "next",
    "react-dom": "next",
    "react-hook-form": "^7.46.2",
    "react-loading": "^2.0.3",
    "styled-components": "^6.0.8",
    "styled-jsx": "^5.1.2",
    "styled-reset": "^4.5.1",
    "use-count-up": "^3.0.1",
    "usehooks-ts": "^3.0.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/node": "^20.7.0",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.0.0",
    "eslint-config-next": "^14.0.0",
    "postcss": "^8.4.30",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.6.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2"
  }
}
